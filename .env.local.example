# Clerk Authentication Keys
# Get these from https://dashboard.clerk.com/last-active?path=api-keys

# Public key (safe to expose in client-side code)
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_key_goes_here

# Secret key (server-side only, never expose to client)
CLERK_SECRET_KEY=sk_key_goes_here

# Optional: Custom URLs for Clerk hosted pages
NEXT_PUBLIC_CLERK_SIGN_IN_URL=/sign-in
NEXT_PUBLIC_CLERK_SIGN_UP_URL=/sign-up
NEXT_PUBLIC_CLERK_AFTER_SIGN_IN_URL=/
NEXT_PUBLIC_CLERK_AFTER_SIGN_UP_URL=/